    function applySetting(setting, choice, conditions)
        local settings = readfile("moslordsettings.cfg");
        local upd1;
        for i, v in pairs(conditions) do
            if string.find(settings, setting..": "..v) then
                upd1 = string.gsub(settings, setting..": "..v, setting..": "..choice);
            end;
        end;
        if not string.find(settings, setting) then
            upd1 = settings.. setting..": "..choice.."\n";
        end;
        
        writefile("moslordsettings.cfg", upd1);
    end
    for i = 0,650 do
        if string.find(readfile("moslordsettings.cfg"), "HitboxSize: "..i) then
            properties.Hsize = i-2;
        end;
        if string.find(readfile("moslordsettings.cfg"), "RejoinMaxPlayers: "..i) then
            properties.maxplayersInServer = i;
        end;
        if string.find(readfile"moslordsettings.cfg", "KillLoop: "..i) then
            properties.killLoop = i;
        end;
    end;
  
    
