    local refreshdropdownbutton3 = playertab:Button("REFRESH DROPDOWN", function()
        inventoryplayersDropdown:Clear();
        inventoryPlayersTable = {};
        for i,v in pairs(game.Players:GetPlayers()) do
            
            table.insert(inventoryPlayersTable, v.Name);
            inventoryplayersDropdown:Add(v.Name);
        end;

    end);
    inventoryplayersDropdown:Clear();
