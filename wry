    local showplayerbutton = playertab:Button("SHOW PLAYER INVENTORY", function()
        if properties.playerToShowInventory == "None" then lib:Notification("PLAYERS", "CHOOSE A PLAYER FIRST", "OK"); return; end;
        local playerStats = game:GetService"ReplicatedStorage":FindFirstChild("Stats"..properties.playerToShowInventory);

        if not playerStats then
            lib:Notification("PLAYERS", "PLAYER NOT FOUND", "OK");
            return false;
        end;
        if not playerStats:FindFirstChild("Inventory") then
            lib:Notification("PLAYERS", "PLAYER INVENTORY NOT FOUND", "OK");
            return false;
        end;

        local inventory = playerStats:FindFirstChild"Inventory".Inventory.Value;
        inventoryLabel.Text = inventory;
        inventoryLabel.TextSize = 17;
        inventoryLabel.TextScaled = true;
        inventoryLabel.Size = UDim2.new(1,0,1,0);
        inventoryLabel.Parent.Size = UDim2.new(0,350,0,1000);
        inventoryLabel.Parent.Parent.CanvasSize = UDim2.new(0, 0, 0, 1000);
        return true;
    end);
    local copyplayerbutton=  playertab:Button("COPY PLAYER INVENTORY", function()
        if properties.playerToShowInventory == "None" then lib:Notification("PLAYERS", "CHOOSE A PLAYER FIRST", "OK"); return; end;
        local playerStats = game:GetService"ReplicatedStorage":FindFirstChild("Stats"..properties.playerToShowInventory);

        if not playerStats then
            lib:Notification("PLAYERS", "PLAYER NOT FOUND", "OK");
            return false;
        end;
        if not playerStats:FindFirstChild("Inventory") then
            lib:Notification("PLAYERS", "PLAYER INVENTORY NOT FOUND", "OK");
            return false;
        end;

        local inventory = playerStats:FindFirstChild"Inventory".Inventory.Value;
        setclipboard(inventory);
    end);


